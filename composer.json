{
    "name": "picturae-mike/joomla-installer",
    "type": "project",
    "description": "Command-line tool to install or update Joomla websites including Docker",
    "keywords": ["joomla", "cli",
        "__docker", "tools"],
    "homepage": "https://github.com/picturae-mike/joomla-installer",
    "license": "MIT",
    "version": "0.4.9",
    "authors": [
        {
            "name": "Picturae",
            "email": "m.vandenberg@picturae.com"
        }
    ],
    "config": {
        "platform-check": false
    },
    "require": {
        "php": ">=8.0",
        "composer/composer": "2.6.x-dev",
        "symfony/console": "^6",
        "symfony/filesystem": "6.1",
        "vlucas/phpdotenv": "dev-master",
        "grasmash/symfony-console-spinner": "dev-main",
        "nesbot/carbon": "3.x-dev",
        "ext-mysqli": "*",
        "ifsnop/mysqldump-php": "dev-master",
        "envms/fluentpdo": "2.2.*",
        "guzzlehttp/guzzle": "7.7.x-dev"
    },
    "autoload": {
        "psr-4": {
            "PicturaeInstaller\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "bin": ["bin/joomla-installer.php"],
    "require-dev": {
        "phpunit/phpunit": "^10"
    }
}
